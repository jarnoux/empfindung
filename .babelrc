{
  "presets": [
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["babel-plugin-transform-builtin-extend", { "globals": ["Error"] }]
  ],
  "env": {
    "development": {
      "sourceMaps": "inline"
    },
    "test": {
      "plugins": [
        "__coverage__"
      ]
    }
  }
}
